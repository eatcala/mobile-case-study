{
  "name": "cala",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "precommit": "lint-staged",
    "env:staging": "node scripts/set-environment.js staging",
    "env:dev": "node scripts/set-environment.js development",
    "env:prod": "node scripts/set-environment.js production",
    "_android": "adb reverse tcp:8081 tcp:8081 && adb reverse tcp:4000 tcp:4000 && react-native run-android",
    "_ios": "react-native run-ios",
    "ios": "npm run env:staging && npm run _ios",
    "android": "npm run env:dev && npm run _android",
    "deploy:internal:android": "npm run env:staging && cd ./android && fastlane internal",
    "deploy:internal:ios": "npm run env:staging && cd ./ios && fastlane internal",
    "deploy:alpha:android": "npm run env:staging && cd ./android && fastlane alpha",
    "deploy:beta:android": "npm run env:prod && cd ./android && fastlane beta",
    "deploy:beta:ios": "npm run env:prod && cd ./ios && fastlane beta",
    "deploy:prod:android": "npm run env:prod && cd ./android && fastlane production",
    "deploy:prod:ios": "npm run env:prod && cd ./ios && fastlane production"
  },
  "dependencies": {
    "@apollo/client": "^3.1.3",
    "@apollo/react-hooks": "^3.1.3",
    "@react-native-community/async-storage": "^1.12.0",
    "@react-native-community/clipboard": "^1.2.3",
    "@react-native-community/masked-view": "^0.1.10",
    "@twotalltotems/react-native-otp-input": "^1.3.11",
    "dayjs": "^1.8.28",
    "graphql": "^14.5.8",
    "jwt-decode": "^2.2.0",
    "prop-types": "^15.7.2",
    "react": "16.9.0",
    "react-content-loader": "^4.3.4",
    "react-native": "^0.63.2",
    "react-native-bootsplash": "^3.1.2",
    "react-native-device-info": "^5.6.3",
    "react-native-fast-image": "^8.3.2",
    "react-native-flags": "^1.0.0",
    "react-native-gesture-handler": "^1.5.3",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-material-textfield": "git+https://github.com/eatcala/react-native-material-textfield.git",
    "react-native-modal": "^11.5.4",
    "react-native-pure-jwt": "^2.2.1",
    "react-native-safe-area-context": "^3.1.3",
    "react-native-screens": "^2.10.1",
    "react-native-svg": "^10.1.0",
    "react-native-svg-transformer": "^0.14.3",
    "react-native-webview": "^10.3.2",
    "react-navigation": "^4.4.0",
    "react-navigation-stack": "^2.8.2",
    "styled-components": "^5.2.0",
    "tipsi-stripe": "^8.0.0-beta.10"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "@react-native-community/eslint-config": "^0.0.5",
    "babel-jest": "^24.9.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "^0.63.0",
    "prettier": "^1.19.1",
    "react-test-renderer": "16.9.0"
  },
  "jest": {
    "preset": "react-native"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ]
  }
}
